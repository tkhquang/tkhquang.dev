{
  "name": "tkhquang.dev",
  "private": true,
  "scripts": {
    "prebuild": "chmod +x ./scripts/prebuild.sh && ./scripts/prebuild.sh",
    "add:lambda": "cd src/lambda && yarn add",
    "install:lambda": "cd src/lambda && yarn",
    "install:lib": "cd lib/gridsome-custom-plugin && yarn",
    "build": "yarn prebuild $HEAD && yarn install:lib && gridsome build",
    "build:lambda": "netlify-lambda build src/lambda",
    "build:test": "concurrently \"yarn dev:lambda\" \"yarn build && http-server dist --proxy http://127.0.0.1:9000\"",
    "dev:site": "gridsome develop",
    "dev:lambda": "netlify-lambda serve src/lambda",
    "dev": "concurrently \"yarn dev:lambda\" \"yarn dev:site\"",
    "explore": "gridsome explore"
  },
  "dependencies": {
    "@gridsome/plugin-google-analytics": "^0.1.2",
    "@gridsome/plugin-sitemap": "^0.4.0",
    "@gridsome/remark-prismjs": "^0.5.0",
    "@gridsome/source-filesystem": "^0.6.2",
    "@gridsome/source-graphql": "^0.1.0",
    "@gridsome/transformer-remark": "^0.6.4",
    "@noxify/gridsome-plugin-remark-embed": "^1.3.1",
    "axios": "^0.21.1",
    "clipboard-polyfill": "^3.0.1",
    "commentbox.io": "^2.0.0",
    "core-js": "^3.6.5",
    "dayjs": "^1.10.4",
    "fusioncharts": "^3.16.0",
    "gridsome": "^0.7.23",
    "gridsome-plugin-gtm": "^0.1.1",
    "gridsome-plugin-remark-prismjs-all": "^0.3.5",
    "gridsome-remark-figure-caption": "^1.1.3",
    "netlify-cms": "^2.10.55",
    "prismjs": "^1.21.0",
    "slugify": "^1.4.5",
    "tailwindcss": "^1.7.3",
    "tsparticles": "^1.17.8",
    "typeface-merriweather": "^0.0.72",
    "typeface-montserrat": "^0.0.75",
    "vue-fusioncharts": "^3.1.0",
    "vue-icon": "^2.1.2",
    "vue-progressbar": "^0.7.5",
    "vue-simple-accordion": "^0.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-gridsome": "^1.5.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "http-proxy-middleware": "^1.0.5",
    "http-server": "^0.12.3",
    "netlify-lambda": "^2.0.1",
    "node-sass": "^4.14.1",
    "postcss": "^8.2.10",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "purgecss-whitelister": "^2.4.0",
    "sass-loader": "^9.0.3",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.18.0"
